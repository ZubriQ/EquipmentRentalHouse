<UserControl x:Class="EquipmentRentalHouse.UserControls.Catalogues"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EquipmentRentalHouse.UserControls"
             mc:Ignorable="d">
    <StackPanel HorizontalAlignment="Center" Margin="5" VerticalAlignment="Center">
        <Button x:Name="btnDevices" Template="{StaticResource Submit}" Content="Devices"
                    Margin="5" Width="120" Height="28" Click="btnDevices_Click"
                        />
        <Button x:Name="btnPositions" Template="{StaticResource Submit}" Content="Positions"
                    Margin="5" Width="120" Height="28" Click="btnPositions_Click"
                        />
        <Button x:Name="btnManufacturers" Template="{StaticResource Submit}" Content="Manufacturers"
                    Margin="5" Width="120" Height="28" Click="btnManufacturers_Click"
                        />
        <Button x:Name="btnStreets" Template="{StaticResource Submit}" Content="Streets"
                    Margin="5" Width="120" Height="28" Click="btnStreets_Click"
                        />
    </StackPanel>
</UserControl>
